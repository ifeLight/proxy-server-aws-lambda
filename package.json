{
  "name": "proxy-server-aws",
  "version": "1.0.0",
  "description": "A proxy server that works on AWS lambda",
  "main": "build/index.js",
  "scripts": {
    "build": "npx tsc",
    "start": "npm run build && node build/index.js",
    "serverless": "npm run build && npx claudia generate-serverless-express-proxy --express-module build/app",
    "deploy": "npm run serverless && npx claudia create --handler lambda.handler --deploy-proxy-api --region eu-central-1",
    "test": "cross-env NODE_ENV=test npx mocha --require ts-node/register src/**/*.test.ts",
    "saving-variable": "TS_NODE_FILES=true"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/ifeLight/proxy-server-aws-lambda.git"
  },
  "keywords": [
    "proxy",
    "server",
    "aws",
    "lambda"
  ],
  "author": "Ifedayo Karim",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/ifeLight/proxy-server-aws-lambda/issues"
  },
  "homepage": "https://github.com/ifeLight/proxy-server-aws-lambda#readme",
  "dependencies": {
    "aws-serverless-express": "^3.4.0",
    "dotenv": "^16.1.4",
    "dotenv-yaml": "^0.1.4",
    "express": "^4.18.2",
    "http-proxy-middleware": "^2.0.6"
  },
  "devDependencies": {
    "@types/chai": "^4.3.4",
    "@types/express": "^4.17.17",
    "@types/mocha": "^10.0.1",
    "@types/node": "^18.11.18",
    "@types/supertest": "^2.0.12",
    "chai": "^4.3.7",
    "claudia": "^5.14.1",
    "cross-env": "^7.0.3",
    "mocha": "^10.2.0",
    "supertest": "^6.3.3",
    "ts-mocha": "^10.0.0",
    "ts-node": "^10.9.1",
    "typescript": "^4.9.4"
  }
}
